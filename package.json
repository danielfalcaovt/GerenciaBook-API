{
  "name": "backend",
  "version": "1.6.2",
  "main": "index.js",
  "scripts": {
    "dev": "node ./dist/main/server.js",
    "debug": "nodemon -L --watch --inspect=0.0.0.0:9222 --nolazy ./dist/main/server.js",
    "up": "npm run build && docker-compose up",
    "down": "docker-compose down",
    "test": "jest --passWithNoTests --silent --noStackTrace --runInBand --forceExit",
    "test:int": "npm run test -- -c ./jest-integration-config.js",
    "test:verbose": "jest --passWithNoTests --forceExit",
    "build": "rimraf ./dist && tsc",
    "test:staged": "npm test -- --findRelatedTests ./src"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "nodemon": "^3.1.4",
    "pg": "^8.12.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.5.4",
    "validator": "^13.12.0"
  },
  "lint-staged": {
    "*.ts": [
      "eslint './src/**/*.ts' --fix",
      "npm run test:staged"
    ]
  },
  "overrides": {
    "eslint": "^9.8.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/pg": "^8.11.6",
    "@types/supertest": "^6.0.2",
    "@types/validator": "^13.12.0",
    "eslint": "^9.8.0",
    "git-commit-msg-linter": "^5.0.8",
    "globals": "^15.9.0",
    "husky": "^9.1.4",
    "lint-staged": "^15.2.8",
    "prettier": "^3.3.3",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.4",
    "ts-node": "^10.9.2",
    "typescript-eslint": "^8.0.1"
  }
}
